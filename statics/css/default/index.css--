.video_table tr td .detail{display:none;width: 174px;height: 133px;background: url("../../images/default/arrow_03.png") no-repeat 0 0;text-align: center;font-size: 13px;line-height: 23px;color: #fff;padding:17px 20px 0;position: absolute;left: -72px;top: 53px;z-index:2;word-break:break-all;}
.video_left{width: 672px;}
.video_left video{width: 100%;}
