/*联系我们*/
.lainXi_left{width: 494px;height: 263px;background: url("../../images/default/lianxi_03.jpg") no-repeat 0 0;text-align: center;position: relative;}
.lainXi_left_text{z-index: 10;}
.lianXi_hui{width: 494px;height: 263px;position: absolute;left: 0;top: 0;background: rgba(255,255,255,0.7);transition: all 0.3s;}
.lainXi_left h2{font-size: 33px;line-height: 33px;color: #3c3c3c;z-index: 20;margin-top:85px;font-weight: 500;}
.lainXi_left a{display: inline-block;font-size: 14px;line-height: 38px;height: 38px;background-color: #33a643;color: #fff;border-radius: 20px;width: 150px;margin-top:30px;}
.lainXi_left_text{position: absolute;top: 0;left: 0;width: 494px;}
.lainXi_left:hover .lianXi_hui{background: rgba(255,255,255,0);}
.lainXi_left:hover .lainXi_left_h2{color: #fff;}
.lainXi_title{font-size: 23px;color: #33a643;line-height: 23px;text-align: center;font-weight: bolder;margin: 70px 0 65px 0;}
.lian_four{margin: 0 -44px;}
.lian_four li{width: 180px;text-align: center;float: left;margin: 0 44px;}
.lian_four li .lian_four_top{font-size: 18px;color: #3c3c3c;line-height: 18px;margin-top: 24px;margin-bottom: 7px;}
.lian_four li .lian_four_bot{font-size: 14px;color: #707070;line-height: 21px;}
/*会员中心*/
.center_header{height: 110px;background-color: #fff;border-bottom: 6px solid #33a643;margin-bottom: 30px;}
.center_header span{display: block;width: 1px;height: 33px;background-color: #bfbfbf;margin: 44px 19px 0;}
.center_header h3{font-size: 22px;line-height: 22px;color: #3c3c3c;margin-top: 50px;font-weight: 500;}
.center_header p{font-size: 13px;line-height: 13px;color: #787878;margin-top: 67px;}
.center_header a{color: #787878;margin-left: 32px;}
.center_header:hover a{color: #33a643;}
.center_bot_left{width: 300px;height: 284px;background-color: #fff;}
.center_box{background-color: #33a643;height: 44px;}
.center_box a{display: block;padding-left: 35px;padding-right: 26px;}
.center_box a p{font-size:15px;color: #fff;line-height: 44px;}
.center_box a .center_span{display: block;width: 15px;height: 15px;background: url("../../images/default/center_icon_03.png") no-repeat 0 0;margin-top: 15px;}
.center_bot_left ul li{border-bottom: 1px solid #d7d7d7;padding-left: 35px;padding-right: 26px;}
.center_bot_left ul li a p.center_p{color: #fff;}
.center_bot_left ul li a.on p{color: #33a643;}
.center_bot_left ul li a.on .icon2{color: #33a643;}

.center_bot_left ul li a{display: block;height: 44px;}
.center_bot_left ul li a p{font-size: 15px;color: #707070;line-height: 44px;}
.center_bot_left ul li a .icon2{display: block;font-size: 14px;color: #707070;line-height: 44px;}
.center_bot_left ul li:hover a p{color: #33a643;}
.center_bot_left ul li:hover a .icon2{color: #33a643;}
.center_bot_left ul li:hover a .icon{background: url("../../images/default/center_icon_lv.png") no-repeat 0 0;}
/*右边*/
.center_bot_right{margin-left: 30px;height: 479px;background-color: #fff;border-radius: 4px;width: 870px;margin-bottom: 189px;}
.center_bot_right p{font-size: 15px;color: #3c3c3c;line-height: 53px;border-bottom: 1px solid #e6e6e6;padding-left: 34px;}
