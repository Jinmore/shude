.center_th{width: 480px !important;}
.center_table tr td img{vertical-align: middle;}
.three_shi{font-size: 23px;line-height: 77px;color: #33a643;text-align: center;font-weight: 600;}
.hei{width: 100%;height: 100%;position: fixed;background-color: #000;opacity: 0.7;left: 0;top: 0;display: none;z-index: 40;}

.shi_center video{position:fixed;top: 0;right: 0;bottom: 0;left: 0;margin: auto;z-index: 70;display: none;}
.guan{width: 40px;height: 40px;position: fixed;top: 11%;right: 23%;border-radius: 50%;cursor: pointer;text-align: center;display: none;z-index: 50;}
